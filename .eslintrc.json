{
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "plugin:@typescript-eslint/recommended-requiring-type-checking", "prettier"],
    "parser": "@typescript-eslint/parser",
    "rules": {
        "indent": ["error", 2],
        "quotes": ["error", "single"],
        "no-console": "off",
        "one-var": "off",
        "sort-keys": "off",
        "sort-imports": "off",
        "no-empty": "off",
        "no-underscore-dangle": "error"
    }
}